RECODE pobreza (3=0) (2=1) (1=1) INTO pobre. 

ROC
  FAC1_1  BY  pobre(1)
  /PLOT = CURVE(REFERENCE)
  /CRITERIA = CUTOFF(INCLUDE) TESTPOS(LARGE) DISTRIBUTION(FREE) CI(95)
  /MISSING = EXCLUDE .

